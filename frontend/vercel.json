{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@vercel/static-build" }],
  "env": {
    "VITE_API_BASE": "https://YOUR_RENDER_BACKEND.onrender.com/api/v1",
    "VITE_SCHEMA_URL": "https://YOUR_RENDER_BACKEND.onrender.com/api/schema/"
  },
  "routes": [{ "src": "/(.*)", "dest": "/" }]
}

